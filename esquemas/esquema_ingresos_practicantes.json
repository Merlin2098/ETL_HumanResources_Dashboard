{
  "metadata": {
    "version": "2.0",
    "capa": "gold",
    "fecha_creacion": "2025-01-07",
    "fecha_actualizacion": "2025-01-07",
    "descripcion": "Esquema de datos para Relación de Ingresos - Solo PRACTICANTES va a Gold"
  },
  "hojas": {
    "PRACTICANTES": {
      "descripcion": "Datos de practicantes para análisis de procedencia y gestión de ingresos",
      "schema": {
        "PERIODO": {
          "type": "string",
          "nullable": false,
          "pattern": "^\\d{4}-\\d{2}$",
          "description": "Periodo calculado en formato YYYY-MM"
        },
        "AÑO": {
          "type": "integer",
          "nullable": false,
          "min_value": 2020
        },
        "MES": {
          "type": "integer",
          "nullable": false,
          "min_value": 1,
          "max_value": 12
        },
        "Numero Documento": {
          "type": "string",
          "nullable": false,
          "description": "DNI o documento de identidad del practicante"
        },
        
        "Sexo": {
          "type": "string",
          "nullable": true,
          "allowed_values": ["M", "F"]
        },
        "Cargo": {
          "type": "string",
          "nullable": true
        },
        "Área": {
          "type": "string",
          "nullable": true
        },
        "Jefe Directo": {
          "type": "string",
          "nullable": true
        },
        "Lugar de Trabajo": {
          "type": "string",
          "nullable": true
        },
        "Universidad de Procedencia": {
          "type": "string",
          "nullable": true
        }
      },
      "constraints": {
        "primary_key": ["PERIODO", "Numero Documento"],
        "business_rules": [
          "PERIODO debe ser calculado como AÑO-MES en formato YYYY-MM",
          "Universidad de Procedencia: Si es nulo, transformar a 'POR DEFINIR'"
        ]
      },
      "transformations": {
        "column_naming": "original",
        "date_format": "YYYY-MM-DD",
        "string_trimming": true,
        "null_handling": "explicit"
      }
    }
  },
  "notas_generales": {
    "arquitectura": "Bronze → Silver → Gold (PRACTICANTES)",
    "power_bi": "Conectar a gold/Relacion Ingresos PRACTICANTES Gold.parquet",
    "actualizacion": "Los archivos se sobreescriben para mantener rutas estables"
  }
}