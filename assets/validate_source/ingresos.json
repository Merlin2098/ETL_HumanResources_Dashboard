{
  "id": "ingresos",
  "name": "PDT Relacion de Ingresos Bronze",
  "file_type": {
    "expected_extensions": [".xlsx", ".xlsm", ".xls"]
  },
  "sheet_rules": {
    "required_sheets": ["EMPLEADOS", "PRACTICANTES"],
    "allowed_sheets": ["EMPLEADOS", "PRACTICANTES"],
    "enforce_allowed_sheets": false
  },
  "sheets": {
    "EMPLEADOS": {
      "header_start": {
        "row_index_1_based": 2,
        "column_letter": "B"
      },
      "required_columns_source": {
        "schema_path": "assets/esquemas/esquema_relacion_ingresos.json",
        "sheet_name": "EMPLEADOS",
        "extract": "schema_non_nullable",
        "exclude_columns": ["PERIODO", "SERVICIO", "REGIMEN"],
        "required_columns": [
          "AÑO",
          "MES",
          "DNI",
          "N° DOCUM."
        ]
      }
    },
    "PRACTICANTES": {
      "header_start": {
        "row_index_1_based": 2,
        "column_letter": "A"
      },
      "required_columns_source": {
        "schema_path": "assets/esquemas/esquema_ingresos_practicantes.json",
        "sheet_name": "PRACTICANTES",
        "extract": "schema_non_nullable",
        "exclude_columns": ["PERIODO"],
        "required_columns": [
          "AÑO",
          "MES",
          "Numero Documento"
        ]
      }
    }
  }
}
