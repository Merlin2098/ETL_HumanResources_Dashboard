{
  "id": "bd",
  "name": "BD Bronze",
  "file_type": {
    "expected_extensions": [".xlsx", ".xlsm", ".xls"]
  },
  "sheet_rules": {
    "required_sheets": ["METSO"],
    "allowed_sheets": ["METSO"],
    "enforce_allowed_sheets": false
  },
  "sheets": {
    "METSO": {
      "header_start": {
        "row_index_1_based": 10,
        "column_letter": "A"
      },
      "required_columns_source": {
        "schema_path": "assets/esquemas/esquema_bd.json",
        "extract": "columns_required_true",
        "required_columns": [
          "NUMERO DE DOC",
          "CODIGO SAP2",
          "NOMBRE COMPLETOS",
          "GERENCIA",
          "SEXO",
          "SEDE3",
          "WHITE COLLAR / BLUE COLLAR",
          "Modalidad de Contrato",
          "Fecha de Termino",
          "SERVICIO",
          "REGIMEN DE TRABAJO",
          "FECH_INGR.",
          "FECHA DE NAC."
        ]
      }
    }
  }
}
